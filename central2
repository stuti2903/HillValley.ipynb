import java.util.*;
class instantfoods
{
    void five()
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Welcome to the Instant Foods");
        System.out.println("1.)Noodles");
        System.out.println("2.)Soup");
        System.out.println("3.)Instant Mixes");
        System.out.println("4.)Seasonings");
        int ifa=sc.nextInt();
        switch(ifa)
        {
            case 1:
                System.out.println("Noodles");
                System.out.println("1.Maggi Masala ");
                System.out.println("2.Maggie Atta");
                System.out.println("3.Yippe");
                System.out.println("4.Ching's Manchow");
                System.out.println("5.Ching's Veggie");
                int noa=sc.nextInt();
                switch(noa)
                {
                    case 1:System.out.println("You have selected Maggie Masala");
                        System.out.println("The cost is Rs 12");
                        System.out.println("Enter the quantity:");
                        int mmq=sc.nextInt();
                        int mm= 12*mmq;
                        break;
                    case 2:System.out.println("You have selected Maggie Atta");
                        System.out.println("The cost is Rs 20");
                        System.out.println("Enter the quantity:");
                        int maq=sc.nextInt();
                        int ma= 50*maq;
                        break;
                    case 3:System.out.println("You have selected Yippe");
                        System.out.println("The cost is Rs 20");
                        System.out.println("Enter the quantity:");
                        int yiq=sc.nextInt();
                        int yi= 20*yiq;
                        break;
                    case 4:System.out.println("You have selected Ching's Manchow");
                        System.out.println("The cost is Rs 45)");
                        System.out.println("Enter the quantity:");
                        int cmq=sc.nextInt();
                        int cm= 45*cmq;
                        break;
                    case 5:System.out.println("You have selected Ching's Veggie");
                        System.out.println("The cost is Rs 30");
                        System.out.println("Enter the quantity:");
                        int cvq=sc.nextInt();
                        int cv= 30*cvq;
                        break;
                }
                break;

            case 2:
                System.out.println("SOUP");
                System.out.println("1.Tomato");
                System.out.println("2.Manchow");
                System.out.println("3.Corn");
                System.out.println("4.Hot n Sour");
