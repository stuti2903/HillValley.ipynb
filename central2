import java.util.*;
class central2
{
    void main()
    {
        Scanner sc=new Scanner(System.in);
        veggies obj1=new veggies();
        dairy obj2=new dairy();
        munchy obj3=new munchy();
        drinks obj4=new drinks();
        instantfoods obj5=new instantfoods();
        pulses obj6=new pulses();
        
        System.out.println("Welcome to the SMALL BASKET ");
        System.out.println("::::::::::::::::::::::");
        System.out.println("Choose a category of products");
        System.out.println("1.)Veggies and Fruits");
        System.out.println("2.)Dairy and Breakfast");
        System.out.println("3.)Munchies");
        System.out.println("4.)Cold Drinks");
        System.out.println("5.)Instant Foods");
        System.out.println("6.)Pulses");
        int a=sc.nextInt();
        switch(a)
        {
            case 1:
                obj1.one();
            break;
            
            case 2:
                obj2.two();
            break;
            
            case 3:
                obj3.three();
            break;
            
            case 4:
                obj4.four();
            break;
            
            case 5:
                obj5.five();
            break;
            
            case 6:
                obj6.six();
            break;
            
            default:System.out.println("Category does not exist :(");
            break;
        }
    }
}
