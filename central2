                        int br= 150*brq;
                        break;
                    default:System.out.println("Does not exist");

                }
                break;

            case 3:
                System.out.println("DAALS");
                System.out.println("1.Toor");
                System.out.println("2.Moong");
                System.out.println("3.Masoor");
                System.out.println("4.Channa");
                System.out.println("5.Rajma");
                int raa=sc.nextInt();
                switch(raa)
                {
                    case 1:System.out.println("You have selected Toor");
                        System.out.println("The cost is Rs 165(1kg)");
                        System.out.println("Enter the quantity:");
                        int toq=sc.nextInt();
                        int to= 165*toq;
                        break;                       
                    case 2:System.out.println("You have selected Mong");
                        System.out.println("The cost is Rs 113(1kg)");
                        System.out.println("Enter the quantity:");
                        int moq=sc.nextInt();
                        int mo= 113*moq;
                        break;
                    case 3:System.out.println("You have selected Massor");
                        System.out.println("The cost is Rs 145,1piece(1kg)");
                        System.out.println("Enter the quantity:");
                        int msq=sc.nextInt();
                        int ms= 145*msq;
                        break;
                    case 4:System.out.println("You have selected Channa");
                        System.out.println("The cost is Rs 190(1 kg)");
                        System.out.println("Enter the quantity:");
                        int chq=sc.nextInt();
                        int chd= 190*chq;
                        break;
                    case 5:System.out.println("You have selected Rajma");
                        System.out.println("The cost is Rs 180(1 kg)");
                        System.out.println("Enter the quantity:");
                        int raq=sc.nextInt();
                        int ra= 180*raq;
                        break;
                    default:System.out.println("Does not exist");
                }
                break;
        }
    }
}
